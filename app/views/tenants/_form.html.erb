<div class="tenant_edit">
	
<%= form_for(@tenant) do |f| %>
  <% if @tenant.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@tenant.errors.count, "error") %> prohibited this tenant from being saved:</h2>

      <ul>
      <% @tenant.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

<table>
 <tr>
 	<td>
  <div class="field">
    <%= f.label :first_name %><br />
    <%= f.text_field :first_name %>
  </div></td>
	<td>
  <div class="field">
    <%= f.label :last_name %><br />
    <%= f.text_field :last_name %>
  </div>
    </td>
 </tr>
 <tr>
	<td>
  <div class="field">
    <%= f.label :telephone %><br />
    <%= f.text_field :telephone, :in => 1...10 %>
  </div>
   </td>
   <td>
  <div class="field">
    <%= f.label :mobile_phone %><br />
    <%= f.text_field :mobile_phone %>
  </div>
   </td>
 </tr>
 <tr>
   <td>
  <div class="field">
	<%= f.label :address %><br />
	<%= f.text_field :address %>
  </div>
   </td>
   <td>	
  <div class="field">
	<%= f.label :city %><br />
	<%= f.text_field :city%>
  </div>
</td>
</tr>
<tr>
	<td>
  <div class="field">
	<%= f.label :postcode %><br />
	<%= f.text_field :postcode %>
  </div>
    </td>
 </tr>
<tr>
	<td>
  <div class="field">
    <%= f.label :job_title %><br />
    <%= f.text_field :job_title %>
  </div>
</td>
</tr>
<tr>
	<td>
  <div class="field">
    <%= f.label :gender %><br />
    <%= f.text_field :gender %>
  </div>
</td>
</tr>

<tr>
<td>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
</td>
</tr>
</table>
</div>