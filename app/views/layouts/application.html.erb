<!DOCTYPE html>
<html>
<head>
  <title>LMS | Landlord Management System <%= @title %></title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
  <%= render 'layouts/stylesheets' %>
  <!-- <%= render :partial => 'shared/includes' %> -->

</head>
<body>
<div class="container">
	<%=render 'layouts/header' %>
	<%=render 'layouts/side' %>
	<!-- <section class="round"> -->
	<% flash.each do |key, value| %>
		<div class ="flash <%= key %>"><%= value %></div>
	 <% end %>
	<%= yield %>
		
	<!-- </section> -->
	<%=render 'layouts/footer' %>
	<!-- <h1><%= debug(params) if Rails.env.development? %></h1> -->
	
</div>

</body>
</html>
